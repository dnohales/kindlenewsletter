{% spaceless %}
<ul class="generic-list stream-list">
	{% if category is defined %}
		{% include 'EorKnlBundle:Reader:_streamRow.html.twig' with {'stream': category, 'is_category': true} %}
	{% endif %}
	{% for s in streams %}
		{% include 'EorKnlBundle:Reader:_streamRow.html.twig' with {'stream': s} %}
	{% endfor %}
</ul>
{% endspaceless %}